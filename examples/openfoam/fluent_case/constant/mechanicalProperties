FoamFile
{
    version     2.4;
    format      ascii;
    class       dictionary;
    object      materialProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


materials
{
    mat_01
    {
        materialModel    linear;      //orthotropic, anisotropic
        T0       T0 [0 0 0 1 0 0 0] 0;
        
        rho    rho [ 0 0 0 0 0 0 0 ]  7850;
        EX     EX  [ 0 0 0 0 0 0 0 ]  ( 20, 2.1e11, 250, 2.05e11, 500, 1.9e10 )  
        MUX    MUX [ 0 0 0 0 0 0 0 ]  0.3; 
        alpha  alpha [ 0 0 0 0 0 0 0 ] 2.1e-5;
   }

   mat_02
   {
       materialModel    linear    //orthotropic, anisotropic
       
       rho    rho [ 0 0 0 0 0 0 0 ]  7850;
       EX     EX  [ 0 0 0 0 0 0 0 ]  2.1e11;
       MUX    MUX [ 0 0 0 0 0 0 0 ]  0.3;
       alpha  alpha [ 0 0 0 0 0 0 0 ] 2.1e-5;
       C        C [0 2 -2 -1 0 0 0] 434;
       KX       KX [1 1 -3 -1 0 0 0] tabular $KX_TABLE;
    }
}


materialToZone ( mat_01, Zone_01, mat_02, Zone_02 );


