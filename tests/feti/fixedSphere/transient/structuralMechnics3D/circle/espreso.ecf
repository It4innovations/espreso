# ESPRESO Configuration File

DEFAULT_ARGS {
  0    HEXA8;

  1        2;
  2        2;
  3        1;

  4        2;
  5        2;
  6        4;

  7        8;
  8        8;
  9        8;
}

INPUT_TYPE              GENERATOR;
PHYSICS      STRUCTURAL_MECHANICS;

GENERATOR {
  SHAPE   GRID;

  GRID {
    START_X                    0;
    START_Y                    0;
    START_Z                    0;

    LENGTH_X                  .1;
    LENGTH_Y                  .1;
    LENGTH_Z               .0012;

    NODES {
       X0 < 0 ,  0> < 0 , .1> <0 , .0012>;
       X1 <.1 , .1> < 0 , .1> <0 , .0012>;
       Y0 < 0 , .1> < 0 ,  0> <0 , .0012>;
       Y1 < 0 , .1> <.1 , .1> <0 , .0012>;
    }

    FACES {
      CONTACT <0 , .1> <0 , .1> <.0012 , .0012>;
    }

    ELEMENT_TYPE          [ARG0];

    CLUSTERS_X            [ARG1];
    CLUSTERS_Y            [ARG2];
    CLUSTERS_Z            [ARG3];

    DOMAINS_X             [ARG4];
    DOMAINS_Y             [ARG5];
    DOMAINS_Z             [ARG6];

    ELEMENTS_X            [ARG7];
    ELEMENTS_Y            [ARG8];
    ELEMENTS_Z            [ARG9];
  }
}

STRUCTURAL_MECHANICS {
  LOAD_STEPS   1;

  MATERIALS {
    1 {
      DENS                         15220;

      ELASTICITY_PROPERTIES {
        MATERIAL_MODEL   NEO_HOOKEAN_CMP;

        MIXY                         0.3;
        EX                          7E10;
      }
    }
  }

  MATERIAL_SET {
    ALL_ELEMENTS   1;
  }

  LOAD_STEPS_SETTINGS {
    1 {
      DURATION_TIME    2e-3;
      TYPE        TRANSIENT;
      MODE        NONLINEAR;
      SOLVER           FETI;

      FETI {
        METHOD          TOTAL_FETI;
        PRECONDITIONER   DIRICHLET;
        ITERATIVE_SOLVER     MPRGP;
        REGULARIZATION    ANALYTIC;
        PRECISION            1e-06;
        MAX_ITERATIONS        1000;
      }

      NONLINEAR_SOLVER {
        MAX_ITERATIONS                     15;
        REQUESTED_DISPLACEMENT_RESIDUAL  1e-3;
      }

      TRANSIENT_SOLVER {
        TIME_STEP          2e-3;

        NUMERICAL_DAMPING   0.1;

        DAMPING {
          RAYLEIGH {
            TYPE   DIRECT;
            DIRECT_DAMPING {
              STIFFNESS    0;
              MASS         0;
            }
          }
        }
      }

      DISPLACEMENT {
        X0   { X 0 ; Y 0 ; Z 0; }
        X1   { X 0 ; Y 0 ; Z 0; }
        Y0   { X 0 ; Y 0 ; Z 0; }
        Y1   { X 0 ; Y 0 ; Z 0; }
      }

      FIXED_SPHERE {
        CONTACT {
          GAP                        .060;
          RADIUS                     .045;
          CENTER {
            X 0.05 + 0.04;
            Y 0.05;
            Z 0.045 + 0.0012 - 0.12 * TIME;
          }
        }
      }
    }

    2 {
      DURATION_TIME       1;
      TYPE        TRANSIENT;
      MODE        NONLINEAR;
      SOLVER           FETI;

      FETI {
        METHOD          TOTAL_FETI;
        PRECONDITIONER   DIRICHLET;
        ITERATIVE_SOLVER     MPRGP;
        REGULARIZATION    ANALYTIC;
        PRECISION            1e-08;
        MAX_ITERATIONS         500;
      }

      NONLINEAR_SOLVER {
        MAX_ITERATIONS                     15;
        REQUESTED_DISPLACEMENT_RESIDUAL  1e-3;
      }

      TRANSIENT_SOLVER {
        TIME_STEP          1e-2;

        NUMERICAL_DAMPING   0.1;

        DAMPING {
          RAYLEIGH {
            TYPE   DIRECT;
            DIRECT_DAMPING {
              STIFFNESS    0;
              MASS         0;
            }
          }
        }
      }

      DISPLACEMENT {
        X0   { X 0 ; Y 0 ; Z 0; }
        X1   { X 0 ; Y 0 ; Z 0; }
        Y0   { X 0 ; Y 0 ; Z 0; }
        Y1   { X 0 ; Y 0 ; Z 0; }
      }

      FIXED_SPHERE {
        CONTACT {
          GAP                        .0060;
          RADIUS                     .0045;
          CENTER {
            X 0.05 + 0.04 * COS(2 * PI * TIME);
            Y 0.05 + 0.04 * SIN(2 * PI * TIME);
            Z 0.0045;
          }
        }
      }
    }
  }
}

OUTPUT {
#  RESULTS_STORE_FREQUENCY           NEVER;
  MONITORS_STORE_FREQUENCY   EVERY_SUBSTEP;

  STORE_RESULTS         ALL;

  MONITORING {
    1 {
      REGION        ALL_NODES;
      STATISTICS          MIN;
      PROPERTY   DISPLACEMENT;
    }

    2 {
      REGION        ALL_NODES;
      STATISTICS          MAX;
      PROPERTY   DISPLACEMENT;
    }

    3 {
      REGION        ALL_NODES;
      STATISTICS          AVG;
      PROPERTY   DISPLACEMENT;
    }
  }
}
